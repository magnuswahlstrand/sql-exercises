<table>
    <tr>
        {{- /*gotype: github.com/magnuswahlstrand/sql-exercises/functions/db.Response*/ -}}
        {{range .Headers}}
            <td class="p-2 font-bold bg-slate-200">{{.}}</td>
        {{end}}
    </tr>
    {{range .Rows}}
        <tr class="odd:bg-white even:bg-slate-50">
            {{range .}}
                <td class="p-2">{{.}}</td>
            {{end}}
        </tr>
    {{end}}
</table>
